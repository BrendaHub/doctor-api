<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://code.alibabatech.com/schema/dubbo
		http://code.alibabatech.com/schema/dubbo/dubbo.xsd" default-autowire="byName">
		
	<dubbo:application name="binggou-api-consumer"/>
	
	<dubbo:registry protocol="zookeeper" address="zookeeper1.binggou.com:2181" />
	

	<dubbo:reference interface="com.doctor330.cloud.server.news.NewsService" id="newsService" check="false"/>
	<dubbo:reference interface="com.doctor330.cloud.server.common.IRedisService" id="redisService" check="false"/>
	<dubbo:reference interface="com.doctor330.cloud.server.message.IMessageService" id="messageService" check="false"/>

</beans>